{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    { "source": "/chat", "destination": "/chat.html" },
    { "source": "/chat-preview", "destination": "/chat-preview.html" },
    {
      "source": "/((?!assets/|.*\\.[a-zA-Z0-9]+$).*)",
      "destination": "/index.html"
    }
  ]
}
